baseline
baseline: spec
	<baseline>
	spec
		for: #common
		do: [ 
			spec blessing: #baseline.
			spec
				configuration: 'Roassal2'
					with: [ 
							spec
								versionString: #stable;
								repository: 'github://ObjectProfile/Roassal2:pharo7/src' ];
				yourself.
			spec
				package: 'tauC-Model' with: [ spec requires: #('Roassal2') ];
				group: 'default' with: #('tauC-Model') ]
